FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    bottomWall
      {
        type            fixedValue;   
        value           uniform 0;
      }
    topWall
      {
        type            fixedValue;
        value           uniform 0;
      }
    sides1
      {
        type            cyclic;//empty;
      }
    sides2
      {
        type            cyclic;//empty;
      }
    inout1
      {
        type            cyclic;
      }
    inout2
      {
        type            cyclic;
      }
}


